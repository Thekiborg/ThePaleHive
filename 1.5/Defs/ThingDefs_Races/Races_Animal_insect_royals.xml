<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- queen -->

  <ThingDef ParentName="BaseInsect">
    <defName>PaleQueen</defName>
    <label>pale queen</label>
    <description>queen.</description>
    <statBases>
      <MoveSpeed>1.60</MoveSpeed>
      <ComfyTemperatureMin>-40</ComfyTemperatureMin>
      <MarketValue>5000</MarketValue>
      <LeatherAmount>0</LeatherAmount>
      <ArmorRating_Blunt>0.30</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.30</ArmorRating_Sharp>
    </statBases>
	<comps>
	  <li Class="CompProperties_Spawner">
        <thingToSpawn>InsectJelly</thingToSpawn>
        <spawnCount>100</spawnCount>
        <spawnIntervalRange>200</spawnIntervalRange>
        <spawnMaxAdjacent>100</spawnMaxAdjacent>
        <spawnForbidden>false</spawnForbidden>
        <saveKeysPrefix>jelly</saveKeysPrefix>
      </li>
    </comps>
    <uiIconScale>1.15</uiIconScale>
    <tools>
      <li>
        <label>head claw</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>BeetleLikeWithClaw</body>
      <baseHungerRate>0.35</baseHungerRate>
      <baseBodySize>4.0</baseBodySize>
      <baseHealthScale>3.6</baseHealthScale>
      <meatLabel>insect meat</meatLabel>
      <lifeExpectancy>6</lifeExpectancy>
      <wildness>0.4</wildness>
      <trainability>Intermediate</trainability>
      <lifeStageAges>
        <li>
          <def>EusocialInsectLarva</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>EusocialInsectJuvenile</def>
          <minAge>0.03</minAge>
        </li>
        <li>
          <def>EusocialInsectAdult</def>
          <minAge>0.2</minAge>
          <soundWounded>Pawn_BigInsect_Pain</soundWounded>
          <soundDeath>Pawn_BigInsect_Death</soundDeath>
          <soundCall>Pawn_BigInsect_Call</soundCall>
          <soundAngry>Pawn_BigInsect_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalInsect</li>
    </tradeTags>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
    <defName>PaleQueen</defName>
    <label>pale queen</label>
    <race>PaleQueen</race>
    <combatPower>150</combatPower>
    <canArriveManhunter>false</canArriveManhunter>
    <ecoSystemWeight>0.50</ecoSystemWeight>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Animals/Larva/PaleLarva</texPath>
          <drawSize>1.2</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Animals/Larva/Dessicated_Larva</texPath>
          <drawSize>1.2</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Animals/Queen/PaleQueen</texPath>
          <drawSize>1.6</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Animals/Queen/Dessicated_Queen</texPath>
          <drawSize>1.6</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Animals/Queen/PaleQueen</texPath>
          <drawSize>2.5</drawSize>
          <shadowData>
            <volume>(0.5, 0.7, 0.43)</volume>
            <offset>(0,0,-0.1)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Animals/Queen/Dessicated_Queen</texPath>
          <drawSize>2.5</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>
  
